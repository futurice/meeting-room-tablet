<?xml version="1.0" encoding="utf-8"?>

    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:id="@android:id/background">
            <shape
                android:shape="line">
                <stroke
                    android:color="@color/seekBar_Free_ProgressBackgroundColor"
                    android:width="@dimen/seek_bar_thickness"/>
            </shape>
        </item>

        <item android:id="@android:id/progress">
            <clip>
                <shape
                    android:shape="line">
                    <stroke
                        android:color="@color/seekBar_Free_Progress"
                        android:width="@dimen/seek_bar_thickness"/>
                </shape>
            </clip>
        </item>

        <item android:id="@android:id/secondaryProgress">
            <clip>
                <shape
                    android:shape="line">
                    <stroke
                        android:color="@color/seekBar_Free_SecondaryProgress"
                        android:width="@dimen/seek_bar_thickness"/>
                </shape>
            </clip>
        </item>
    </layer-list>
